{
    "manifest_version": 3,
    "name": "MDN Search Hub",
    "description": "Search Hub for MDN",
    "version": "1.0",
    "icons": {
      "16": "mdn-icon.png",
      "48": "mdn-icon.png",
      "128": "mdn-icon.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "mdn-icon.png"
    },
    "permissions": [
     "storage"
  ],
  "host_permissions":[
    "https://*/*",
    "http://*/*"
  ]
}
  